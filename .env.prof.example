# Profiling Environment Configuration
# Copy this file to .env.prof and customize as needed

# Django Configuration
DJANGO_DEBUG=True
SECRET_KEY=your-profiling-secret-key-here-change-in-production
JWT_SECRET_KEY=your-profiling-jwt-secret-key-here-change-in-production
ALLOWED_HOSTS=localhost,127.0.0.1,0.0.0.0

# Database Configuration (PostgreSQL)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_HOST=db
POSTGRES_PORT=5432
POSTGRES_DB=drf_starter_profiling

# Direct URLs for Docker Compose
DATABASE_URL=postgresql://postgres:postgres@db:5432/drf_starter_profiling
REDIS_URL=redis://redis:6379/0

# Web Server
WEB_PORT=8101

# Project Information
PROJECT_NAME=DRF Starter Profiling
PROJECT_DESCRIPTION=Profiling environment for DRF starter project
PROJECT_VERSION=1.0.0-profiling

# Contact Information
CONTACT_NAME=Profiling Team
CONTACT_EMAIL=profiling@example.com
CONTACT_URL=https://profiling.example.com

# Email Configuration (Console backend for profiling)
EMAIL_HOST=localhost
EMAIL_PORT=1025
EMAIL_USE_TLS=False
EMAIL_HOST_USER=
EMAIL_HOST_PASSWORD=
EMAIL_FRONTEND_DOMAIN=http://localhost:8101

# Theme Colors
THEME_PRIMARY_COLOR=#6a0dad
THEME_ACCENT_COLOR=#4b0082

# Environment
DJANGO_ENV=prof
DJANGO_SETTINGS_MODULE=config.django.profiling

# Profiling Configuration
PROFILING_ENABLED=True
PYINSTRUMENT_ENABLED=True
