[run]
source = 
    accounts
    config
    utils
omit = 
    */tests/*
    */migrations/*
    */__pycache__/*
    */venv/*
    */.venv/*
    */env/*
    */.env/*
    manage.py
    */settings/*
    */urls.py
    */wsgi.py
    */asgi.py
    */celery.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
    def main\(\):
    if TYPE_CHECKING:
    from __future__ import annotations
