{
  "auth": {
    "login_endpoint": "/api/v1/auth/jwt/create/",
    "email_field": "email",
    "password_field": "password"
  },
  "endpoint_groups": {
    "main_endpoints": [
      {
        "endpoint": "/api/v1/auth/jwt/create/",
        "method": "POST",
        "auth": false,
        "title": "üîê JWT Token Creation",
        "data": {
          "email": "admin@katesthe-core.com",
          "password": "admin"
        },
        "description": "Authenticate user and generate JWT access/refresh tokens"
      },
      {
        "endpoint": "/api/v1/auth/jwt/destroy/",
        "method": "POST",
        "auth": true,
        "title": "üîê JWT Token Logout",
        "description": "Logout user and invalidate JWT tokens"
      },
      {
        "endpoint": "/api/v1/auth/users/activation/dummy/uid/dummy/token/",
        "method": "POST",
        "auth": false,
        "title": "‚úâÔ∏è User Account Activation",
        "description": "Activate user account using UID and token"
      }
    ],
    "api_root": [
      {
        "endpoint": "/",
        "method": "GET",
        "auth": false,
        "title": "üè† Root Redirect",
        "description": "Application root - redirects to Django admin",
        "enabled": false
      },
      {
        "endpoint": "/api/v1/",
        "method": "GET",
        "auth": false,
        "title": "üöÄ API v1 Root",
        "description": "API version 1 root endpoint",
        "enabled": false
      }
    ],
    "api_docs": [
      {
        "endpoint": "/api/schema/",
        "method": "GET",
        "auth": false,
        "title": "üìã API Schema (OpenAPI)",
        "description": "Machine-readable API specification in OpenAPI 3.0 format"
      },
      {
        "endpoint": "/api/schema/docs/",
        "method": "GET",
        "auth": false,
        "title": "üìñ Swagger UI Documentation",
        "description": "Interactive API documentation with Swagger UI"
      },
      {
        "endpoint": "/api/schema/redoc/",
        "method": "GET",
        "auth": false,
        "title": "üìö ReDoc Documentation",
        "description": "Clean, responsive API documentation interface"
      }
    ],
    "profiling_tools": [
      {
        "endpoint": "/silk/",
        "method": "GET",
        "auth": false,
        "title": "üïµÔ∏è Silk Profiling Dashboard",
        "description": "Django application profiling and performance monitoring"
      },
      {
        "endpoint": "/rosetta/",
        "method": "GET",
        "auth": false,
        "title": "üåê Rosetta Translation Interface",
        "description": "Web-based interface for translating Django applications"
      }
    ]
  },
  "profiling": {
    "base_url": "http://127.0.0.1:8101",
    "profile_parameter": "profile",
    "profile_value": "1",
    "concurrent_requests": 3,
    "requests_per_endpoint": 2,
    "request_timeout": 30,
    "user_agent": "DjangoProfiler/1.0",
    "retry_attempts": 3,
    "retry_delay": 1,
    "follow_redirects": true,
    "verify_ssl": false
  },
  "docker": {
    "container_name": "web_profiling",
    "container_profiles_path": "/app/profiles",
    "host_profiles_path": "./profiles",
    "sync_interval": 5
  },
  "output": {
    "format": "html",
    "include_metadata": true,
    "include_request_headers": true,
    "include_response_headers": true,
    "include_timing": true,
    "include_sql_queries": true,
    "include_template_rendering": true
  },
  "filters": {
    "exclude_endpoints": [
      "/silk/static/",
      "/static/admin/fonts/",
      "/media/favicon.ico",
      "/robots.txt",
      "/favicon.ico"
    ],
    "include_only_authenticated": false,
    "include_only_public": false,
    "min_response_time": 0,
    "max_response_time": 30
  },
  "notifications": {
    "enable_email": false,
    "enable_webhook": false,
    "email_recipients": [],
    "webhook_url": "",
    "thresholds": {
      "slow_request": 2.0,
      "error_rate": 0.1,
      "memory_usage": 100
    }
  }
}
